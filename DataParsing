

## Ohio Primary ##
TotalforBernie = sum(unique(columbus[c(grep("Sanders",columbus$V1),grep("Bernie",columbus$V1), grep("FeelTheBern", columbus$V1)),]$V2))
TotalforHillary = sum(unique(columbus[c(grep("Hillary",columbus$V1),grep("Clinton",columbus$V1), grep("ImWithHer", columbus$V1)),]$V2))
TotalforTrump = sum(unique(columbus[c(grep("Donald",columbus$V1),grep("Trump",columbus$V1), grep("MakeAmericaGreatAgain", columbus$V1)),]$V2))
TotalforCruz = sum(unique(columbus[c(grep("Ted",columbus$V1),grep("Cruz",columbus$V1), grep("CruzCrew", columbus$V1)),]$V2))
TotalforRubio = sum(unique(columbus[c(grep("Marco",columbus$V1),grep("Rubio",columbus$V1)),]$V2))
TotalforKasich = sum(unique(columbus[grep("Kasich",columbus$V1),]$V2))
tallys = c(TotalforCruz,TotalforKasich, TotalforRubio, TotalforTrump, TotalforHillary, TotalforBernie)
colors = c("red", "red", "red", "red", "blue", "blue")
barplot(tallys, main = "Ohio Primary", ylab = "Number of Mentions", xlab = "Canidates", names.arg = c("Cruz", "Kasich", "Rubio", "Trump", "Clinton", "Sanders"), col = colors)
oh = matrix(tallys)
colnames(oh) <- "Total Mentions"
rownames(oh) <- c("Cruz", "Kasich", "Rubio", "Trump", "Clinton", "Sanders")
oh
totaltweetsoh = columbus[grep("statuses_count", columbus$V1), ]$V2

## Illinois Primary ##
TotalforBernie2 = sum(unique(chicago[c(grep("Sanders",chicago$V1),grep("Bernie",chicago$V1), grep("FeelTheBern", chicago$V1)),]$V2))
TotalforHillary2 = sum(unique(chicago[c(grep("Hillary",chicago$V1),grep("Clinton",chicago$V1), grep("ImWithHer", chicago$V1)),]$V2))
TotalforTrump2 = sum(unique(chicago[c(grep("Donald",chicago$V1),grep("Trump",chicago$V1), grep("MakeAmericaGreatAgain", chicago$V1)),]$V2))
TotalforCruz2 = sum(unique(chicago[c(grep("Ted",chicago$V1),grep("Cruz",chicago$V1), grep("CruzCrew", chicago$V1)),]$V2))
TotalforRubio2 = sum(unique(chicago[c(grep("Marco",chicago$V1),grep("Rubio",chicago$V1)),]$V2))
TotalforKasich2 = sum(unique(chicago[grep("Kasich",chicago$V1),]$V2))
tallys2 = c(TotalforCruz2,TotalforKasich2, TotalforRubio2, TotalforTrump2, TotalforHillary2, TotalforBernie2)
colors = c("red", "red", "red", "red", "blue", "blue")
barplot(tallys2, main = "Illinois Primary", ylab = "Number of Mentions", xlab = "Canidates", names.arg = c("Cruz", "Kasich", "Rubio", "Trump", "Clinton", "Sanders"), col = colors)
il = matrix(tallys2)
colnames(il) <- "Total Mentions"
rownames(il) <- c("Cruz", "Kasich", "Rubio", "Trump", "Clinton", "Sanders")
il
totaltweetsil = chicago[grep("statuses_count", chicago$V1), ]$V2

## Florida Primary ##
TotalforBernie3 = sum(unique(jacksonville[c(grep("Sanders",jacksonville$V1),grep("Bernie",jacksonville$V1), grep("FeelTheBern", jacksonville$V1)),]$V2))
TotalforHillary3 = sum(unique(jacksonville[c(grep("Hillary",jacksonville$V1),grep("Clinton",jacksonville$V1), grep("ImWithHer", jacksonville$V1)),]$V2))
TotalforTrump3 = sum(unique(jacksonville[c(grep("Donald",jacksonville$V1),grep("Trump",jacksonville$V1), grep("MakeAmericaGreatAgain", jacksonville$V1)),]$V2))
TotalforCruz3 = sum(unique(jacksonville[c(grep("Ted",jacksonville$V1),grep("Cruz",jacksonville$V1), grep("CruzCrew", jacksonville$V1)),]$V2))
TotalforRubio3 = sum(unique(jacksonville[c(grep("Marco",jacksonville$V1),grep("Rubio",jacksonville$V1)),]$V2))
TotalforKasich3 = sum(unique(jacksonville[grep("Kasich",jacksonville$V1),]$V2))
tallys3 = c(TotalforCruz3,TotalforKasich3, TotalforRubio3, TotalforTrump3, TotalforHillary3, TotalforBernie3)
colors = c("red", "red", "red", "red", "blue", "blue")
barplot(tallys3, main = "Florida Primary", ylab = "Number of Mentions", xlab = "Canidates", names.arg = c("Cruz", "Kasich", "Rubio", "Trump", "Clinton", "Sanders"), col = colors)
fl = matrix(tallys3)
colnames(fl) <- "Total Mentions"
rownames(fl) <- c("Cruz", "Kasich", "Rubio", "Trump", "Clinton", "Sanders")
fl
totaltweetsfl = jacksonville[grep("statuses_count", jacksonville$V1), ]$V2

