## Total Mentions  ##
TotalTed = sum(TotalforCruz, TotalforCruz2, TotalforCruz3, TotalforCruz4, TotalforCruz5, TotalforCruz6)
TotalTrump = sum(TotalforTrump, TotalforTrump2, TotalforTrump3, TotalforTrump4, TotalforTrump5, TotalforCruz6)
TotalKasich = sum(TotalforKasich, TotalforKasich2, TotalforKasich3, TotalforKasich4, TotalforKasich5, TotalforKasich6)
TotalBerns = sum(TotalforBernie, TotalforBernie2, TotalforBernie3, TotalforBernie4, TotalforBernie5, TotalforBernie6)
TotalHills = sum(TotalforHillary, TotalforHillary2, TotalforHillary3, TotalforHillary4, TotalforHillary5, TotalforHillary6)
count = c(TotalTed, TotalKasich,TotalTrump, TotalHills, TotalBerns)
barplot(count, col = colors2, main = "Total Mentions",  xlab = "Canidates", ylab = "Number of Mentions", names.arg = c("Cruz", "Kasich", "Trump", "Clinton", "Sanders"))

