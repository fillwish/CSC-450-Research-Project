## Utah Primary ##
TotalforBernie5 = sum(unique(saltlakecity[c(grep("Sanders",saltlakecity$V1),grep("Bernie",saltlakecity$V1), grep("FeelTheBern", saltlakecity$V1)),]$V2))
TotalforHillary5 = sum(unique(saltlakecity[c(grep("Hillary",saltlakecity$V1),grep("Clinton",saltlakecity$V1), grep("ImWithHer", saltlakecity$V1)),]$V2))
TotalforTrump5 = sum(unique(saltlakecity[c(grep("Donald",saltlakecity$V1),grep("Trump", saltlakecity$V1), grep("MakeAmericaGreatAgain", saltlakecity$V1)),]$V2))
TotalforCruz5 = sum(unique(saltlakecity[c(grep("Ted",saltlakecity$V1),grep("Cruz",saltlakecity$V1), grep("CruzCrew", saltlakecity$V1)),]$V2))
TotalforRubio5 = sum(unique(saltlakecity[c(grep("Marco",saltlakecity$V1),grep("Rubio",saltlakecity$V1)),]$V2))
TotalforKasich5 = sum(unique(saltlakecity[grep("Kasich",saltlakecity$V1),]$V2))
tallys5 = c(TotalforCruz5,TotalforKasich5,TotalforTrump5, TotalforHillary5, TotalforBernie5)
colors2 = c("red", "red", "red","blue", "blue")
barplot(tallys5, main = "Utah Primary", ylab = "Number of Mentions", xlab = "Canidates", names.arg = c("Cruz", "Kasich", "Trump", "Clinton", "Sanders"), col = colors2)
ut = matrix(tallys5)
colnames(ut) <- "Total Mentions"
rownames(ut) <- c("Cruz", "Kasich","Trump", "Clinton", "Sanders")
ut
totaltweetsut = saltlakecity[grep("statuses_count", saltlakecity$V1), ]$V2

## Wisconsin Primary ##
TotalforBernie6 = sum(unique(milwaukee[c(grep("Sanders",milwaukee$V1),grep("Bernie",milwaukee$V1), grep("FeelTheBern", milwaukee$V1)),]$V2))
TotalforHillary6 = sum(unique(milwaukee[c(grep("Hillary",milwaukee$V1),grep("Clinton",milwaukee$V1), grep("ImWithHer", milwaukee$V1)),]$V2))
TotalforTrump6 = sum(unique(milwaukee[c(grep("Donald", milwaukee$V1),grep("Trump", milwaukee$V1), grep("MakeAmericaGreatAgain", milwaukee$V1)),]$V2))
TotalforCruz6 = sum(unique(milwaukee[c(grep("Ted",milwaukee$V1),grep("Cruz",milwaukee$V1), grep("CruzCrew", milwaukee$V1)),]$V2))
TotalforKasich6 = sum(unique(milwaukee[grep("Kasich",milwaukee$V1),]$V2))
tallys6 = c(TotalforCruz6,TotalforKasich6,TotalforTrump6, TotalforHillary6, TotalforBernie6)
colors2 = c("red", "red", "red","blue", "blue")
barplot(tallys6, main = "Wisconsin Primary", ylab = "Number of Mentions", xlab = "Canidates", names.arg = c("Cruz", "Kasich", "Trump", "Clinton", "Sanders"), col = colors2)
wi = matrix(tallys6)
